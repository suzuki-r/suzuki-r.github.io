Cell[TextData[{
 "Since Wick contractions can be taken in any order, we move the 1st field in \
the 1st argument to the 1st position in the 2nd argument,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{":", 
    RowBox[{
     RowBox[{
      SubscriptBox["A", "1"], "..."}], " ", 
     SubscriptBox["A", "m"], " ", 
     RowBox[{"(", "w", ")"}]}], ":", " ", ":", 
    RowBox[{
     RowBox[{
      SubscriptBox["B", "1"], "..."}], " ", 
     SubscriptBox["B", "n"], " ", 
     RowBox[{"(", "z", ")"}]}], ":", " ", 
    RowBox[{"~", " ", 
     RowBox[{":", 
      RowBox[{
       RowBox[{
        SubscriptBox["A", "2"], "..."}], " ", 
       SubscriptBox["A", "m"], " ", 
       RowBox[{"(", "w", ")"}]}], ":", " ", ":", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          SubscriptBox["A", "1"], "(", "w", ")"}], 
         SubscriptBox["B", "1"]}], "..."}], " ", 
       SubscriptBox["B", "n"], " ", 
       RowBox[{"(", "z", ")"}]}], ":", 
      RowBox[{"+", " ", 
       RowBox[{
        UnderscriptBox["\[Sum]", "i"], 
        RowBox[{"Wick", "[", 
         RowBox[{
          RowBox[{
           SubscriptBox["A", "1"], "(", "w", ")"}], ",", 
          RowBox[{
           SubscriptBox["B", "i"], "(", "w", ")"}]}], "]"}]}]}], ":", 
      RowBox[{
       RowBox[{
        SubscriptBox["A", "2"], "..."}], " ", 
       SubscriptBox["A", "m"], " ", 
       RowBox[{"(", "w", ")"}]}], ":", " ", ":", 
      RowBox[{
       RowBox[{
        SubscriptBox["B", "1"], "..."}], " ", 
       SubscriptBox["B", "n"], " ", 
       RowBox[{"(", "z", ")"}]}], ":"}]}]}], TraditionalForm]]]
